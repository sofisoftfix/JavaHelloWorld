pipeline {
	agent any
    environment { 
        NEW_VERSION='1.3.0'
        //SERVER_CREDENTIALS= credentials('server-credentials')

    }
	stages {
		stage("Build") {
			steps {
                echo 'deployin application.............'
				// get code from repository
				withCredentials([
                    usernamepassword(credentials: 'sever-credentials', usernameVariable:USER, passwordVariable:PWD)
                ]){
                    bat "some scripts ${USER} ${PWD}"
                }
				}
			}
		}
	}
